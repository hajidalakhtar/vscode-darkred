		"titleBar.inactiveBackground": "#110022",
		"titleBar.inactiveForeground": "#cccccc",
		// badge
		"badge.background": "#ffc600",
		"badge.foreground": "#000",
		//button
		"button.background": "#003364",
		"button.foreground": "#f9f7f7",
		"button.hoverBackground": "#002446",
		// status bar
		"statusBar.background": "#002446",
		"statusBar.foreground": "#f9f7f7",
		"statusBar.noFolderBackground": "#2b2e4a",
		// activity
		"activityBar.background": "#001122",
		"activityBar.foreground": "#004455",
		// editor
		"editor.background": "#001122",
		"editor.foreground": "#f9f7f7",
		// sidebar
		"sideBarTitle.foreground": "#f9f7f7",
		"sideBar.background": "#001122",
		"sideBar.foreground": "#f9f7f7",
		"sideBar.border": "#393e46",
		"sideBarSectionHeader.background": "#002446",
		"sideBarSectionHeader.foreground": "#f9f7f7",
		// list
		"list.hoverBackground": "#002446",
		"list.focusBackground": "#004455",
		// dropdown
		"dropdown.background": "#004455",
		"dropdown.foreground": "#f9f7f7",
		"settings.dropdownBackground": "#001122",
		// input
		"input.background": "#001122",
		"input.border": "#521262",
		"input.foreground": "#f9f7f7",
		"input.placeholderForeground": "#eeeeee",
		"inputOption.activeBorder": "#521262",
		// tab
		"tab.activeBackground": "#002446",
		"tab.activeForeground": "#f9f7f7",
		"tab.inactiveBackground": "#112233",
		// editor header
		"editorGroupHeader.tabsBackground": "#110022",
		// notification
		"notificationCenterHeader.background": "#004455",
		"notifications.background": "#001122"

		
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#52616b"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#30e3ca"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#11999e"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
